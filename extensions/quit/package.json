{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "quit",
  "title": "Quit Apps",
  "description": "Quit all or any of the running applications",
  "icon": "command-icon.png",
  "author": "kusthi",
  "categories": [
    "System"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "quit",
      "title": "Quit",
      "description": "Lists out all running applications to quit",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.58.0",
    "run-applescript": "^6.1.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "2.8.8",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
